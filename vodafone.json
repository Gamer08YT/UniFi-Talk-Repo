{
  "name": "Vodafone",
  "description": "SIP template for Vodafone Deutschland.",
  "fields": [
    {
      "name": "proxy",
      "description": "Proxy is <Area code>.sip.arcor.de"
    },
    {
      "name": "password"
    },
    {
      "name": "username",
      "description": "Username is <Area code> + <Phone number>"
    },
    {
      "name": "from-domain",
      "value": "sip.arcor.de"
    }
  ],
  "acl-whitelist": [
    {
      "host": "0.0.0.0",
      "cidr": "0"
    }
  ],
  "settings": {
    "static-signaling-port": false,
    "codec": "PCMA"
  }
}
