    {
      "name": "dtmfmode",
      "value": "rfc2833",
      "description": "Wichtig, damit Tastentöne richtig übertragen werden. Fonial macht das nur nach rfc2833."
    },
    {
      "name": "password"
    },
    {
      "name": "register",
      "value": "true"
    },
    {
      "name": "username",
      "description": "Username bspw. foXXXXXXXX_XX"
    },
    {
      "name": "extension",
      "value": "auto_to_user",
      "description": "Wichtig, sonst sind eingehende Anrufe nicht möglich."
    },
    {
      "name": "retry-seconds",
      "value": "20",
      "description": "Wenn der SIP Client sich anmeldet, fliegt er teilweise sonst laut Fonials Logging direkt wieder raus. Damit erzwingen wir reconnects."
    },
    {
      "name": "register-proxy",
      "value": "proxy01.sip.plusnet.de",
      "description": "Beim Trunking ist es immer der proxy01. Bei den normalen Fonial SIP Anschlüssen (kein Trunking) bitte den Proxy beachten, welcher bei den SIP Zugangsdaten mit angegeben werden."
    },
    {
      "name": "register-transport",
      "value": "udp"
    }
